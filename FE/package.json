{
  "name": "kolsherut_fe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "index.js",
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "scripts": {
    "dev": "vite",
    "copy-configs": "npx shx cp ./public/config.json ./src/services/initialize/offlineConfigAndStrings/config.json && npx shx cp ./public/strings.json ./src/services/initialize/offlineConfigAndStrings/strings.json",
    "generate-assets": "npm run generate-pwa-assets && tsc -b",
    "prebuild": "npm run copy-configs && npm run generate-assets",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal public/icon.svg",
    "docker:build": "npm run build && npx cross-var docker build -t kolsherut_fe:$npm_package_version .",
    "docker:save": "npx cross-var docker save -o ../tars/kolsherut_fe.$npm_package_version.tar kolsherut_fe:$npm_package_version",
    "tar": "npm run docker:build && npm run docker:save"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/poppins": "^5.2.5",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@reduxjs/toolkit": "^2.6.1",
    "axios": "^1.7.9",
    "cross-var": "^1.1.0",
    "jss": "^10.10.0",
    "ol": "^10.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-jss": "^10.10.0",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/node": "^22.13.14",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vite-pwa/assets-generator": "^0.2.6",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "pwa-asset-generator": "^8.0.4",
    "shx": "^0.4.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0",
    "vite-plugin-pwa": "^0.21.1"
  }
}
